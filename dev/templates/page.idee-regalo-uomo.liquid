
{% comment %}
<div class="module-wrapper">
  <div class="module module-default-page">
    <div class="module-header page-header">
      <div class="module-title page-title">
        <h1>{{ page.title }}</h1>
      </div>
    </div>
    {% unless page.content == blank %}
      <div class="module-body page-body rte">
        {{ page.content }}
      </div>
    {% endunless %}
  </div>
</div>
{% endcomment %}

<style>
  .module-body > div > a.button {
  	display: none !important;
  }
</style>

<div class="fc-js-trigger fc-idee-uomo">

  {% section 'idee-uomo-bg_mob_img-1' %}
  
  {% section 'featured-on-w' %}
  
  {% section 'corporate-gifts-text-column-1-ita' %}

  {% section 'idee-uomo-bg_mob_img-2' %}  
  
  <!-- use cases -->
  {% section 'idee-uomo-images-with-text' %}

  {% section 'module-technical-spec-ita' %}

  {% section 'idee-uomo-testimonial' %}

  {% section 'idee-uomo-cta' %}

