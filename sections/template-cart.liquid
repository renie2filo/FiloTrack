<script>
  function fc_invoice_requested_changed(checkbox) {
    if (checkbox.checked) {
      $(".fc_invoice_type_select").show().find("select").val(
        $(".fc_invoice_type_select").find("select option:selected").val()
      ).trigger('change')
    } else {
      $(".fc_invoice_type_select").hide().find("select")
      /*.val($(".fc_invoice_type_select select option:first").val())
      $(".fc_invoice_type_select").find("select option").removeAttr("selected");
      $(".fc_invoice_type_select").find("select option:first-of-type").attr("selected", "selected");*/
      $(".fc_invoice_info_target .fc_invoice_info").remove();
      // it can be hidden / showed and made required, so that the field would retain info 
    }
  }
  function fc_invoice_request_type_changed(sel) {
    const sel_value = sel.value;
    const sel_value_selector = sel_value ? "." + sel_value : ".none";
    //console.log("SEL VALUE: " + sel_value);
    $(".fc_invoice_info_target .fc_invoice_info").remove();
    $(".fc_invoice_info_target").append($("div" + sel_value_selector + " .fc_invoice_info").clone());
  }
  $("#request-invoice").change();
  function fc_vat_number_keyup(inp) {
    $("input.vat_number").val(inp.value);
    $("input.cod_fisc").val(inp.value);
  }
  function fc_cod_fisc_keyup(inp) {
    $("input.cod_fisc").val(inp.value);
  }
  function fc_cod_dest_o_pec_keyup(inp) {
    $("input.cod_dest_o_pec").val(inp.value);
  }
  function fc_quantity_input_changed() {
    console.log("FOUND " + $(".button.cart-button.cart-update-button").length);
    $(".button.cart-button.cart-update-button").click();
  }
</script>

