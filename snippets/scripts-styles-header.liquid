<script type="text/javascript">
  function fc_get_img_url(url, w, h) {
    {% comment %}//console.log("> > > " + url);{% endcomment %}
    if (!h && !w) {
      return url;
    }
    h = h ? Math.ceil(h/100)*100 : '';
    w = w ? Math.ceil(w/100)*100 : '';
    var x = url.lastIndexOf('.');
    return url.substring(0,x) + '_' + w + 'x' + h + url.substring(x, url.length);
  }

  get_uid_or_random = () => {
    try {
      if (localStorage.getItem("uid")) {
        return localStorage.getItem("uid");
      }
      const uid = (5000000000000 - Date.now()) + "_FF" +
      Math.random().toString(36).substring(2, 6) +
      Math.random().toString(36).substring(2, 6);
      localStorage.setItem("uid", uid);
      return uid;
    } catch (err) {
      return (5000000000000 - Date.now()) + "_FF" +
      Math.random().toString(36).substring(2, 6) +
      Math.random().toString(36).substring(2, 6);
    }
  }
</script>

